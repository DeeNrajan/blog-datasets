<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/sec#"
     xml:base="http://example.org/sec"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">

    <owl:Ontology rdf:about="http://example.org/sec"/>

    <!-- Classes -->
    <owl:Class rdf:about="#Company"/>
    <owl:Class rdf:about="#Investor"/>
    <owl:Class rdf:about="#SecFiling"/>
    <owl:Class rdf:about="#OwnershipStatement"/>
    <owl:Class rdf:about="#FinancialStatement"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="#hasFiling">
        <rdfs:domain rdf:resource="#Company"/>
        <rdfs:range rdf:resource="#SecFiling"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#filingBy">
        <rdfs:domain rdf:resource="#SecFiling"/>
        <rdfs:range rdf:resource="#Investor"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasOwnershipStatement">
        <rdfs:domain rdf:resource="#SecFiling"/>
        <rdfs:range rdf:resource="#OwnershipStatement"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasFinancialStatement">
        <rdfs:domain rdf:resource="#SecFiling"/>
        <rdfs:range rdf:resource="#FinancialStatement"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#holdsSharesIn">
        <rdfs:domain rdf:resource="#Investor"/>
        <rdfs:range rdf:resource="#Company"/>
    </owl:ObjectProperty>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="#companyName">
        <rdfs:domain rdf:resource="#Company"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#investorName">
        <rdfs:domain rdf:resource="#Investor"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#filingDate">
        <rdfs:domain rdf:resource="#SecFiling"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#sharePercentage">
        <rdfs:domain rdf:resource="#OwnershipStatement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#financialItem">
        <rdfs:domain rdf:resource="#FinancialStatement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#financialValue">
        <rdfs:domain rdf:resource="#FinancialStatement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>

</rdf:RDF>
