<?xml version="1.0"?>

<!DOCTYPE Ontology [
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
]>

<Ontology xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://example.org/vc"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     ontologyIRI="http://example.org/vc">
    <Prefix name="" IRI="http://www.w3.org/2002/07/owl#"/>
    <Prefix name="owl" IRI="http://www.w3.org/2002/07/owl#"/>
    <Prefix name="rdf" IRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
    <Prefix name="xsd" IRI="http://www.w3.org/2001/XMLSchema#"/>
    <Prefix name="rdfs" IRI="http://www.w3.org/2000/01/rdf-schema#"/>

    <!-- Classes -->
    <Declaration>
        <Class IRI="#Company"/>
    </Declaration>
    <Declaration>
        <Class IRI="#Person"/>
    </Declaration>
    <Declaration>
        <Class IRI="#Product"/>
    </Declaration>
    <Declaration>
        <Class IRI="#Industry"/>
    </Declaration>
    <Declaration>
        <Class IRI="#Location"/>
    </Declaration>

    <!-- Object Properties -->
    <Declaration>
        <ObjectProperty IRI="#hasRole"/>
    </Declaration>
    <Declaration>
        <ObjectProperty IRI="#hasInvestment"/>
    </Declaration>
    <Declaration>
        <ObjectProperty IRI="#hasPartnership"/>
    </Declaration>
    <Declaration>
        <ObjectProperty IRI="#producesProduct"/>
    </Declaration>
    <Declaration>
        <ObjectProperty IRI="#operatesInIndustry"/>
    </Declaration>
    <Declaration>
        <ObjectProperty IRI="#locatedIn"/>
    </Declaration>

    <!-- Data Properties -->
    <Declaration>
        <DataProperty IRI="#companyName"/>
    </Declaration>
    <Declaration>
        <DataProperty IRI="#personName"/>
    </Declaration>
    <Declaration>
        <DataProperty IRI="#role"/>
    </Declaration>
    <Declaration>
        <DataProperty IRI="#investmentAmount"/>
    </Declaration>
    <Declaration>
        <DataProperty IRI="#partnershipDetails"/>
    </Declaration>
    <Declaration>
        <DataProperty IRI="#productName"/>
    </Declaration>
    <Declaration>
        <DataProperty IRI="#industryName"/>
    </Declaration>
    <Declaration>
        <DataProperty IRI="#locationName"/>
    </Declaration>

    <!-- Object Property Characteristics -->
    <ObjectPropertyDomain>
        <ObjectProperty IRI="#hasRole"/>
        <Class IRI="#Person"/>
    </ObjectPropertyDomain>
    <ObjectPropertyRange>
        <ObjectProperty IRI="#hasRole"/>
        <Class IRI="#Company"/>
    </ObjectPropertyRange>

    <ObjectPropertyDomain>
        <ObjectProperty IRI="#hasInvestment"/>
        <Class IRI="#Person"/>
    </ObjectPropertyDomain>
    <ObjectPropertyRange>
        <ObjectProperty IRI="#hasInvestment"/>
        <Class IRI="#Company"/>
    </ObjectPropertyRange>

    <ObjectPropertyDomain>
        <ObjectProperty IRI="#hasPartnership"/>
        <Class IRI="#Company"/>
    </ObjectPropertyDomain>
    <ObjectPropertyRange>
        <ObjectProperty IRI="#hasPartnership"/>
        <Class IRI="#Company"/>
    </ObjectPropertyRange>

    <ObjectPropertyDomain>
        <ObjectProperty IRI="#producesProduct"/>
        <Class IRI="#Company"/>
    </ObjectPropertyDomain>
    <ObjectPropertyRange>
        <ObjectProperty IRI="#producesProduct"/>
        <Class IRI="#Product"/>
    </ObjectPropertyRange>

    <ObjectPropertyDomain>
        <ObjectProperty IRI="#operatesInIndustry"/>
        <Class IRI="#Company"/>
    </ObjectPropertyDomain>
    <ObjectPropertyRange>
        <ObjectProperty IRI="#operatesInIndustry"/>
        <Class IRI="#Industry"/>
    </Object
